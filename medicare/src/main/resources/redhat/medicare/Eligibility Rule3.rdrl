package redhat.medicare;

import java.lang.Number;

rule "Eligibility Rule3"
	dialect "mvel"
	no-loop true
	when
		app : MedicareApplicant( st : state not in ( "MN", "WI" ) )
	then
		modify( app ) {
				setEligibiliy( "N" ), 
				setReasonForNotEligible( " Medicare coverage not available for the state of " + st )
		}
end
