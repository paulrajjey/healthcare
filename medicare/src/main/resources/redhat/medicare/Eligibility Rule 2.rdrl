package redhat.medicare;

import java.lang.Number;

rule "Eligibility Rule 2"
	dialect "mvel"
	no-loop true
	when
		app : MedicareApplicant( age >= 65 , areYouUSCitizen != "Y" )
	then
		app.setEligibiliy( "N" );
		app.setReasonForNotEligible( "Not a US Citizen. Medicare is applicable for only US citizen" );
end
