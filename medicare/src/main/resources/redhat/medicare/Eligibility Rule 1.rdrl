package redhat.medicare;

import java.lang.Number;

rule "Eligibility Rule 1"
	dialect "mvel"
	no-loop true
	when
		app : MedicareApplicant( age < 65 , diabliltyOrSpecialCondition != "Y" )
	then
		app.setEligibiliy( "N" );
		app.setReasonForNotEligible( "Jey-Medicare is for people age 65 and older and those who have special condition or disability." );
end
